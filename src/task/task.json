{
  "id": "b18196d0-d3ed-11e8-ada4-43a6658503da",
  "name": "Lighthouse",
  "friendlyName": "Lighthouse",
  "description": "Lighthouse",
  "author": "Anthony Simmon",
  "helpMarkDown": "Embed Google Lighthouse HTML results in Azure Pipelines",
  "category": "Utility",
  "visibility": [
    "Build",
    "Release"
  ],
  "runsOn": [
    "Agent",
    "DeploymentGroup"
  ],
  "demands": [
    "node.js"
  ],
  "version": {
    "Major": "0",
    "Minor": "1",
    "Patch": "0"
  },
  "minimumAgentVersion": "1.95.0",
  "instanceNameFormat": "Lighthouse $(url)",
  "inputs": [
    {
      "name": "url",
      "type": "string",
      "label": "URL",
      "defaultValue": "",
      "required": true,
      "helpMarkDown": "URL to execute Lighthouse against"
    },
    {
      "name": "args",
      "type": "multiLine",
      "label": "CLI arguments",
      "defaultValue": "",
      "required": false,
      "helpMarkDown": "[CLI arguments](https://github.com/GoogleChrome/lighthouse) to pass to Lighthouse."
    },
    {
      "name": "cwd",
      "type": "filePath",
      "label": "Working Directory",
      "defaultValue": "",
      "required": false,
      "helpMarkDown": "Location of your package.json where the lighthouse npm package is referenced. Defaults to the root of the repository."
    }
  ],
  "execution": {
    "Node": {
      "target": "execute-lighthouse.js",
      "argumentFormat": ""
    }
  }
}